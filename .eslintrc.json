{
    "env": {
        "node": true,
        "mocha": true,
        "browser": true
    },
    "globals": {
        "Vue": true,
        "google": true
    },
    "extends": "airbnb-base",
    "rules":{
        "object-curly-spacing": ["error", "never"],
        "object-curly-newline": ["error", {"ObjectPattern": "never", "ObjectExpression": {"consistent": true}}],
        "import/no-extraneous-dependencies": ["error", {"devDependencies": true}],
        "linebreak-style": "off"
    }
}
